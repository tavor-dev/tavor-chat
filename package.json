{
  "name": "tavor-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev",
    "predev": "convex dev --run init --until-success",
    "build": "tsc -b && vite build",
    "lint": "bun lint:eslint && bun lint:prettier",
    "lint:fix": "bun lint:eslint:fix && bun lint:prettier:fix",
    "lint:eslint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:eslint:fix": "bun lint:eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "typecheck": "tsc -p tsconfig.app.json && tsc -p tsconfig.node.json && tsc -p convex/tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@auth/core": "^0.39.1",
    "@convex-dev/agent": "^0.1.7",
    "@convex-dev/auth": "^0.0.87",
    "@convex-dev/react-query": "^0.0.0-alpha.8",
    "@epic-web/client-hints": "^1.3.5",
    "@medusajs/icons": "^2.8.4",
    "@medusajs/ui": "^4.0.14",
    "@medusajs/ui-preset": "^2.8.4",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@react-email/components": "0.0.22",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-form": "0.26.4",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-router": "^1.121.2",
    "@tanstack/zod-form-adapter": "^0.26.4",
    "@tavor/sdk": "^1.1.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xixixao/uploadstuff": "^0.0.5",
    "class-variance-authority": "^0.7.1",
    "convex": "^1.24.8",
    "convex-helpers": "^0.1.93",
    "cookie": "^0.6.0",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^2.7.3",
    "lucide-react": "^0.515.0",
    "npm-run-all": "^4.1.5",
    "oslo": "^1.2.1",
    "react": "^18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-af1b7da-20250417",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-textarea-autosize": "^8.5.9",
    "remark-gfm": "^4.0.1",
    "resend": "^3.5.0",
    "stripe": "^16.12.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@tanstack/router-devtools": "^1.121.5",
    "@tanstack/router-plugin": "^1.121.4",
    "@types/cookie": "^0.6.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.5.5",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^6.3.5"
  }
}
